<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_company_lingua" model="ir.ui.view">
			<field name="name">view.company.lingua</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form" />
			<field name="arch" type="xml">
				<notebook position="inside">
                    <page string="Lingua logo">
                        <group>
						<field name="head_logo" widget="image" />
						<field name="foot_logo" widget="image" />
						</group>
					</page>
				</notebook>
			</field>
		</record>
	</data>
</openerp> 